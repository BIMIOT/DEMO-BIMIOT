{
  "name": "vue-ifc-example",
  "description": "Example project for using IFC.js within vue.",
  "author": "Joseph Fletcher",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "preserve": "cpy ./node_modules/web-ifc/web-ifc.wasm public/IFCjs cpy ./node_modules/web-ifc-three/IFCWorker.js public/IFCjs cpy ./node_modules/web-ifc-three/IFCWorker.js.map public/IFCjs"
  },
  "main": "background.js",
  "dependencies": {
    "@stomp/stompjs": "^6.1.2",
    "sockjs-client": "^1.6.1",
    "axios": "^1.2.1",
    "core-js": "^3.4.4",
    "lodash": "^4.17.21",
    "readline": "^1.3.0",
    "three": "^0.135.0",
    "unzipit": "^1.3.5",
    "vue": "^2.6.14",
    "vuetify": "^2.5.9",
    "web-ifc-viewer": "^1.0.177"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "cpy-cli": "^3.1.1",
    "npm-force-resolutions": "0.0.10",
    "rollup": "^2.58.0",
    "sass": "^1.42.1",
    "sass-loader": "^10.2.0",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "build": {
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type"
    }
  }
}
